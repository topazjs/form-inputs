{
  "name": "@topazjs/form-inputs",
  "version": "0.0.1",
  "description": "topazjs - Form input components",
  "main": "index.js",
  "private": false,
  "scripts": {
    "test": "jest -u",
    "reset": "rm -rf node_modules;yarn --force",
    "refresh": "npm run prep; npm run reset; npm run build-dist",
    "prep": "sh clean.sh dist",
    "copy-files": "cp -rf src/img src/webfonts dist/",
    "watch": "npm run prep && npm-run-all --parallel js-watch",
    "build": "npm run prep && npm run js-build",
    "build-dist": "npm run prep && npm-run-all --parallel js-build-dist; npm run js-min",
    "js-build": "export NODE_ENV=development;browserify -s ${PWD##*/} -e src/js/App.js -v -d -t envify -t babelify | exorcist app.js.map -u /${PWD##*/}/app.js.map > app.js",
    "js-build-dist": "export NODE_ENV=production;browserify -s ${PWD##*/} -e src/js/App.js -v -d -t envify -t babelify -t uglifyify -o /tmp/app-${PWD##*/}.js",
    "js-build-dist-with-maps": "export NODE_ENV=production;browserify -s ${PWD##*/} -e src/js/App.js -v -d -t envify -t babelify -t uglifyify | exorcist /tmp/app-${PWD##*/}.js.map > /tmp/app-${PWD##*/}.js",
    "js-min-old-with-maps": "uglifyjs /tmp/app-${PWD##*/}.js -m -c --in-source-map /tmp/app-${PWD##*/}.js.map --screw-ie8 --source-map app.js.map --source-map-url /${PWD##*/}/app.js.map > app.js",
    "js-min": "uglifyjs /tmp/app-${PWD##*/}.js -m -c --screw-ie8 > app.js",
    "js-watch": "export NODE_ENV=development;watchify -s ${PWD##*/} -e src/js/App.js -v -d -t envify -t babelify -o 'exorcist app.js.map > app.js'"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
